{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/location-qr/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/location-qr",
        "main": "libs/location-qr/src/index.ts",
        "tsConfig": "libs/location-qr/tsconfig.lib.json",
        "assets": ["libs/location-qr/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/location-qr/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/location-qr"],
      "options": {
        "jestConfig": "libs/location-qr/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
